<div id="<%= dom_id heartbeat %>">
  <p><%= heartbeat.status_code %></p>
  <p><%= heartbeat.request_time %></p>
  <% if false %>
    <% if action_name != "show" %>
      <%= link_to "Show this heartbeat", heartbeat, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      <%= link_to 'Edit this heartbeat', edit_heartbeat_path(heartbeat), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
      <hr class="mt-6">
    <% end %>
  <% end %>
</div>
