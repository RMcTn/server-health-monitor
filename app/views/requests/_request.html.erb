<div id="<%= dom_id request %>">
  <p class="my-5">
    <strong class="block mb-1 font-medium">Status code:</strong>
    <%= request.status_code %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Request time:</strong>
    <%= request.request_time %>
  </p>

  <% if false %>
    <% if action_name != "show" %>
      <%= link_to "Show this request", request, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
      <%= link_to 'Edit this request', edit_request_path(request), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
      <hr class="mt-6">
    <% end %>
  <% end %>
</div>
